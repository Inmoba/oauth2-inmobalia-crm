{
	"name": "inmoba/oauth2-inmobalia-crm",
	"description": "Sign in with InmobaliaCRM OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
	"license": "MIT",
	"keywords": [
		"oauth",
		"oauth2",
		"client",
		"authorization",
		"authorisation",
		"inmobalia-crm",
		"inmobalia",
		"inmoba"
	],
	"require": {
		"php": "^7.4 || ^8.0",
		"ext-json": "*",
		"league/oauth2-client": "^2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^11.5",
		"squizlabs/php_codesniffer": "^3.11.0",
		"mockery/mockery": "^1.6.12"
	},
	"autoload": {
		"psr-4": {
			"Inmobalia\\OAuth2\\Client\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Inmobalia\\OAuth2\\Client\\Test\\": "test/src/"
		}
	},
	"scripts": {
		"check": "phpcs src test example --standard=PSR12 -sp --ignore=*/vendor/*",
		"test": "phpunit"
	}
}